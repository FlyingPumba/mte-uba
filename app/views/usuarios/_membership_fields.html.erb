<tr class="nested-fields">
  <td>
    <%= f.select :role_id, Role.all.map{ |c| [c.name, c.id] }, { include_blank: '-- elija el rol --' }, class: "role_select", id: 'role' %>
  </td>
  <td>
    <%= f.check_box :current %>
  </td>
  <td class="remove">
    <%= link_to_remove_association('' , f,html_options = { class: 'fa fa-trash-o fa-lg' }) %>
  </td>
</tr>
