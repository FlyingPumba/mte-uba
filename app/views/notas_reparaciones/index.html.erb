<p id="notice"><%= notice %></p>

<h1>Listing Nota Reparacions</h1>

<table>
  <thead>
    <tr>
      <th>Numeroserie</th>
      <th>Fecha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notas_reparaciones.each do |nota_reparacion| %>
      <tr>
        <td><%= nota_reparacion.numeroserie %></td>
        <td><%= nota_reparacion.fecha %></td>
        <td><%= link_to 'Show', nota_reparacion %></td>
        <td><%= link_to 'Edit', edit_nota_reparacion_path(nota_reparacion) %></td>
        <td><%= link_to 'Destroy', nota_reparacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nota reparacion', new_nota_reparacion_path %>
