<%= semantic_form_for @comprobante do |f| %>
  <%= f.inputs do %>
    <%= f.input :type %>
    <%= f.input :numeroserie %>
    <%= f.input :fecha %>
    <%= f.input :observation %>
    <%= f.input :comprobantable %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
